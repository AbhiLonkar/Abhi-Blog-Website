<%-include("partials/header")-%>
    <h1 class = "heading">Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="#textField">Title</label>
            <input id="textField" type="text" name="postTitle" class="w-100 form-control" autocomplete = "off">
            <label for="#textPost">Post</label>
            <textarea name="postBody" id="" class="w-100 form-control" rows="5"></textarea>
        </div>
        <button type="submit" name="submit" class="btn btn-primary">Publish</button>
    </form>
<%-include("partials/footer")-%>

